	<!--Вид объекта культурного наследия-->
	<xs:include schemaLocation="dCultural_v01.xsd"/>
	<!--Виды объектов реестра границ-->
	<xs:include schemaLocation="dBoundaryType_v01.xsd"/>
	<!--Виды использования наемного дома-->
	<xs:include schemaLocation="dUseAppartBuild_v02.xsd"/>
	<!--Виды площадей-->
	<xs:include schemaLocation="dArea_v01.xsd"/>
	<!--ПОДСХЕМЫ:-->
	<!--Подсхема АДРЕС-->
	<xs:include schemaLocation="_AddressOut_v06.xsd"/>
	<!--Подсхема КАДАСТРОВАЯ СТОИМОСТЬ-->
	<xs:include schemaLocation="_CadastralCostList_v03.xsd"/>
	<!--Подсхема СВЕДЕНИЯ О ПРИРОДНЫХ ОБЪЕКТАХ-->
	<xs:include schemaLocation="_NaturalObjectsOut_v03.xsd"/>
	<!--Подсхема НАЗНАЧЕНИЕ И ВИД ПОМЕЩЕНИЯ-->
	<xs:include schemaLocation="_AssignationFlat_v03.xsd"/>

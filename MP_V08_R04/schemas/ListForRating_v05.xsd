	<xs:include schemaLocation="P_AllSimpleType_v03.xsd"/>
	<!--КЛАССИФИКАТОРЫ:-->
	<!--Классификатор назначений зданий-->
	<xs:include schemaLocation="dAssBuilding_v02.xsd"/>
	<!--Справочник типов этажей-->
	<xs:include schemaLocation="dTypeStorey_v01.xsd"/>
	<!--Справочник "Виды объектов недвижимости"-->
	<xs:include schemaLocation="dRealty_v04.xsd"/>
	<!--Справочник "Категории земель"-->
	<xs:include schemaLocation="dCategories_v01.xsd"/>
	<!--Справочник "Вид земельного участка"-->
	<xs:include schemaLocation="dParcels_v02.xsd"/>

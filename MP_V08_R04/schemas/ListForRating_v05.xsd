	<xs:include schemaLocation="_ParametersOKSOut_v03.xsd"/>
	<!--======= СХЕМА ПЕРЕЧНЯ ООБЪЕКТОВ НЕДВИЖИМОСТИ. ВЕРСИЯ 5-->
	<xs:element name="ListForRating">
		<xs:annotation>
			<xs:documentation>Перечень объектов недвижимости для целей определения кадастровой стоимости (в соответствии с приказом Росреестра от 06.08.2021г. № 0283)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ListInfo">
					<xs:annotation>
						<xs:documentation>Общие сведения о перечне</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ListType">
								<xs:annotation>
									<xs:documentation>Вид перечня (в соответствии с п.2 Порядка формирования и предоставления перечней ОН): 1 - в соответствии со ст.11 237-ФЗ (предварительный); 2 - в соответствии со ст.13 237-ФЗ; 3 - в соответствии со ст.15 237-ФЗ; 4 - в соответствии со ст.16 237-ФЗ</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:pattern value="1"/>
										<xs:pattern value="2"/>
										<xs:pattern value="3"/>
										<xs:pattern value="4"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Region" type="dRegionsRF">
								<xs:annotation>
									<xs:documentation>Код региона</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ObjectsType">
								<xs:annotation>
									<xs:documentation>Виды объектов недвижимости</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ObjectType" type="dRealty" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Вид объекта недвижимости</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Categories" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Категории земель (п.10 Порядка)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Category" type="dCategories" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Категория земель</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="DateForm" type="xs:date" use="required">
							<xs:annotation>
								<xs:documentation>Дата формирования перечня</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="GUID" type="sGUID" use="required">
							<xs:annotation>
								<xs:documentation>Глобальный уникальный идентификатор перечня</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Objects">
					<xs:annotation>
						<xs:documentation>Перечень объектов недвижимости</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Buildings" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Здания</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Building" type="tBuilding" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Здание</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Constructions" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сооружения</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Construction" type="tConstruction" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сооружение</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Uncompleteds" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Объекты незавершенного строительства</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Uncompleted" type="tUncompleted" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Объект незавершенного строительства</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Flats" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Помещения</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Flat" type="tFlat" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Помещение</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="CarParkingSpaces" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Машино-места</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CarParkingSpace" type="tCarParkingSpace" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Машино-место</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Parcels" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Земельные участки</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Parcel" type="tParcel" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Земельный участок</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Version" type="xs:string" use="required" fixed="05">
				<xs:annotation>
					<xs:documentation>Версия схемы</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--ЗДАНИЯ, СООРУЖЕНИЯ, ОНС, ПОМЕЩЕНИЯ, ЕНК:-->
	<xs:complexType name="tBuilding">
		<xs:annotation>
			<xs:documentation>Здание</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CadastralBlock" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер кадастрового квартала</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentCadastralNumbers" type="tCadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровый номер земельного участка (земельных участков), в пределах которого (которых) расположен данный объект недвижимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrevCadastralNumbers" type="tCadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровые номера объектов недвижимости, из которых образован данный объект недвижимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreviouslyPosted" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ранее учтенный</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectType">
				<xs:annotation>
					<xs:documentation>Вид объекта недвижимости - Здание</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="dRealty">
						<xs:pattern value="002001002000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AssignationBuilding" type="dAssBuilding">
				<xs:annotation>
					<xs:documentation>Назначение здания</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElementsConstruct" type="tElementsConstruct" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Материал наружных стен здания</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExploitationChar" type="tExploitationChar" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Эксплуатационные характеристики</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Floors" type="tFloors" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Количество этажей (в том числе подземных)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Area" type="xs:string">
				<xs:annotation>
					<xs:documentation>Площадь в квадратных метрах</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Location" type="tAddressMain" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Адрес (местоположение)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectPermittedUses" type="tObjectPermittedUses" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вид (виды) разрешенного использования</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CadastralCost" type="tCadastralCostExt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о кадастровой стоимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubBuildings" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о частях здания</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SubBuilding" type="tSubBuildingFlat" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Сведения о части здания</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlatsCadastralNumbers" type="tCadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровые номера помещений, расположенных в объекте недвижимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CarParkingSpacesCadastralNumbers" type="tCadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровые номера машино-мест, расположенных в объекте недвижимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnitedCadastralNumber" type="tFacilityUnited" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о едином недвижимом комплексе, если объект недвижимости входит в состав единого недвижимого комплекса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FacilityCadastralNumber" type="tFacilityUnited" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о предприятии как имущественном комплексе, если объект недвижимости входит в состав предприятия как имущественного комплекса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CulturalHeritage" type="tCulturalHeritage" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о включении объекта недвижимости в единый государственный реестр объектов культурного наследия или об отнесении объекта недвижимости к выявленным объектам культурного наследия</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="CadastralNumber" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Кадастровый номер</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DateCreated" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>Дата присвоения кадастрового номера</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FoundationDate" type="xs:date">
			<xs:annotation>
				<xs:documentation>Дата возникновения оснований для включения сведений об объекте недвижимости в Перечень</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tConstruction">
		<xs:annotation>
			<xs:documentation>Сооружение</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CadastralBlock" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер кадастрового квартала</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentCadastralNumbers" type="tCadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровый номер земельного участка (земельных участков), в пределах которого (которых) расположен данный объект недвижимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrevCadastralNumbers" type="tCadastralNumbers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Кадастровые номера объектов недвижимости, из которых образован данный объект недвижимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreviouslyPosted" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ранее учтенный</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectType">
				<xs:annotation>
					<xs:documentation>Вид объекта недвижимости - Сооружение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="dRealty">
						<xs:pattern value="002001004000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AssignationName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Назначение сооружения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExploitationChar" type="tExploitationChar" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Эксплуатационные характеристики</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Floors" type="tFloors" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Количество этажей (в том числе подземных)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KeyParameters" type="tKeyParameters" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основные характеристики</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Location" type="tAddressMain" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Адрес (местоположение)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectPermittedUses" type="tObjectPermittedUses" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вид (виды) разрешенного использования</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CadastralCost" type="tCadastralCostExt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о кадастровой стоимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubConstructions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о частях сооружения</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
